<script type="text/javascript">
        SP.showRegistrationChart = function () {
            var seriesOptions = SP.getRegistrationChartSeries();
            SP.createRegistrationChart(seriesOptions);
        };
        SP.weeksInfo = {};
        SP.getRegistrationChartSeries = function () {
            var seriesOptions = [],
                series = {},
                weeks = {},
                weekCodes = [],
                weekLabels = {};

            App.cphShellContent_RegistrationProcessOverview_storeRegistrationChart.data.each(function (record, index) {
                weekCodes.push(record.data.XAxisValue);
                weekLabels[record.data.XAxisValue] = record.data.XAxisTooltipValue;
            });
            weekCodes.sort();
            var i = 1;
            for (var w = 0;w<weekCodes.length;w++) { 
                weeks[weekCodes[w]] = i;
                SP.weeksInfo[i] = weekLabels[weekCodes[w]];
                i++;
            }

            App.cphShellContent_RegistrationProcessOverview_storeRegistrationChart.data.each(function (record, index) {
                if (typeof(series[record.data.SerieID]) == "undefined") series[record.data.SerieID] = {name:record.data.SerieTitle, data:[]};
                series[record.data.SerieID].data.push({x:weeks[record.data.XAxisValue], y:record.data.YAxisValue, key:record.data.XAxisValue});
            });
            for (var j in series) {
                seriesOptions.push({
                    name: series[j].name, 
                    data: series[j].data,
                    lineWidth:1,
                    marker: {
                        radius: 2
                    }
                });	 
            }
            return seriesOptions;
        };
        SP.createRegistrationChart = function (seriesOptions) {          
            var panelChart = App.cphShellContent_RegistrationProcessOverview_contRegistrationChart; 
            panelChart.on('afterlayout', SP.resizeRegistrationChart);
            var loadMask;
            try {
                if (typeof(SP.registrationChart) != 'undefined') {
                    SP.registrationChart.destroy();
                }
            } catch(e) { }
            SP.registrationChart = new Highcharts.Chart({
                chart: {
                    animation:true,
                    renderTo: panelChart.body.id,
                    height: panelChart.getHeight(),
                    width: panelChart.getWidth(),
                    zoomType: 'x',
                    resetZoomButton: { position: { align: 'center' } }                    
                }, 
                title: { text:null },
                credits: { enabled: false},	     
                scrollbar: { enabled : false },
                exporting: { enabled : false },
                legend: {
                    borderWidth: 0
                }, 
                xAxis: {
                    labels: { enabled: false },
                    step: 1
                }, 
                yAxis: {
                    min: 0,
                    title: { text:null }
                },
                tooltip: {
                    shared: true,
                    crosshairs: true,
                    /* funkcia, ktorá vyskladá tooltip po zaťažení bodu na grafe */
                    formatter: function(a) {
                        var s = SP.weeksInfo[this.x];                        
                        var tot = 0;
                        Ext.each(this.points, function (i, point) {
                            s += '<br/><span style="color:' + this.series.color + '">' + this.series.name + '</span>: <b>' + this.point.y + '</b>';
                            tot += point.y;
                        });
                        return s;
                    }
                },
                series: seriesOptions
            }, function() { });	
            panelChart.doLayout();
            return true;
        };
        SP.resizeRegistrationChart = function (component) {
            try {
                if (typeof(SP.registrationChart) != 'undefined') {
                    SP.registrationChart.setSize(component.getWidth(), component.getHeight(), false);
                }
            } catch(e) {}
        };
    </script>
</div>

    

    <script type="text/javascript">
            SP.reloadPortal = function (portal) {
                if (SP.portalLoaded) {
                    portal.items.each(function (column) {
                        column.items.each(function (portlet) {
                            SP.reloadWidget(portlet);
                        });
                    });
                }
                SP.portalLoaded = true;
            };
            SP.reloadWidget = function (portlet) {
                var cmp = SP.findComponent(Ext.get(portlet.contentEl).dom);
                if (cmp && cmp.doLayout) setTimeout(function () { cmp.doLayout(); }, 10);
            }
            SP.findComponent = function (el) {
                var cmp = Ext.getCmp(el.id);
                if (cmp) return cmp;
                for (var i = 0; i < el.children.length; i++) {
                    cmp = Ext.getCmp(el.children[i].id);
                    if (cmp) return cmp;
                }
                for (var i = 0; i < el.children.length; i++) {
                    cmp = SP.findComponent(el.children[i]);
                    if (cmp) return cmp;
                }
            };
        </script>

 
        
    </form>                  
