<script type="text/javascript">
        SP.showChart = function () {
            var seriesOptions = SP.getSeries();
            SP.createChart(seriesOptions);
        };
        SP.getColor = function(val) {
            if (val < 50) {
                return "indianred";
            }
            else if( val == 100) {
                return "#669535";
            }
            return "#7CB5EC"
        }
        SP.getData = function() {
            var data = [];
            var store = App.cphShellContent_Atom_storePoints;

            store.data.each(function (record, index) {
                data.push({
                    x: 0,
                    y: record.data.Summary
                })
            });

            return data;
        }
        SP.getSeries = function () {
            var serie = {
                showInLegend: false,
                name: OM.dictionary.get('Title'), 
                data: SP.getData(),
                marker: {
                    enabled: false,
                    radius: 1
                }
            };

            if (serie.data[0]) {
                serie.color = SP.getColor(serie.data[0].y);
            }
            return [serie];
        }
        SP.createChart = function (seriesOptions) {          
            var panelChart = App.cphShellContent_Atom_atomStatusChart; 
            panelChart.on('afterlayout', SP.resizeChart);
            var loadMask;
            try {
                if (typeof(SP.chart) != 'undefined') {
                    SP.chart.destroy();
                }
            } catch(e) { }

            SP.chart = new Highcharts.Chart({
                chart: {
                    animation:true,
                    renderTo: panelChart.body.id,
                    height: panelChart.getHeight(),
                    width: panelChart.getWidth(),
                    zoomType: 'x',
                    resetZoomButton: { position: { align: 'center' } },
                    type: 'bar'
                }, 
                title: { text:null },
                credits: { enabled: false},	     
                scrollbar: { enabled : false },
                exporting: { enabled : false },
                legend: {
                    borderWidth: 0
                }, 
                xAxis: {
                    visible: false,
                    labels: {
                        overflow: 'justify',
                        formatter: function() { return ""; }
                    },
                    tickInterval: 1
                },
                yAxis: {
                    title: {
                        text: OM.dictionary.get('importStatus'),
                        align: 'high'
                    },
                    min: 0,
                    max: 100
                },
                plotOptions: {
                    bar: {
                        dataLabels: {
                            enabled: true,
                            formatter: function () { return this.y.toString().replace('.', ',') + ' % ' },
                            y: -30
                        },
                        pointWidth: 45
                    },
                },
                tooltip: {
                    formatter: function(a) {
                        return OM.dictionary.get('Title') + ": " + this.y + "%";
                    }
                },
                series: seriesOptions
                
            }, function() { });	

            panelChart.doLayout();
            return true;
        };
        SP.resizeChart = function (component) {
            try {
                if (typeof(SP.chart) != 'undefined') {
                    SP.chart.setSize(component.getWidth(), component.getHeight(), false);
                }
            } catch(e) {}
        };
    </script>


