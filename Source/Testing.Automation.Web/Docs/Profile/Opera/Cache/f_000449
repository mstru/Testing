<script type="text/javascript">
        SP.showCollectingStatusChart = function () {
            var seriesOptions = SP.getCollectingStatusSeries();
            SP.createCollectingStatusChart(seriesOptions);
        };
        SP.getCollectingStatusSeries = function () {
            var serie = {
                name: OM.dictionary.get('status'),
                showInLegend: false,
                data: SP.getCollectingStatusData()
            }

            return [serie];
        }
        SP.getCollectingStatusData = function () {
            var data = [];
            var store = App.cphShellContent_CollectingStatus_storeChartCollectingStatusInfo;

            store.data.each(function (record, index) {
                data.push({
                    x: new Date(record.data.Id),
                    y: record.data.Status,
                    color: SP.getCollectingStatusColor(record.data.Status, store.storeId)
                })
            });

            return data; 
        }
        SP.createCollectingStatusChart = function (seriesOptions) {
            var chartOptions = SP.getCollectingStatusOptions();

            var panelChart = App.cphShellContent_CollectingStatus_collectingStatusChart; 
            panelChart.on('afterlayout', SP.resizeCollectingStatusChart);
            var loadMask;
            try {
                if (typeof(SP.collectingStatusChart) != 'undefined') {
                    SP.collectingStatusChart.destroy();
                }
            } catch(e) { }
            
            SP.collectingStatusChart = new Highcharts
                .Chart({
                    chart: {
                        renderTo: panelChart.body.id,
                        marginRight: 25,
                        alignTicks: true,
                        type: chartOptions.chartType,
                    },
                    title: null,
                    xAxis: chartOptions.xAxis,
                    yAxis: chartOptions.yAxis,
                    tooltip: chartOptions.tooltip || '',
                    plotOptions: chartOptions.plotOptions,
                    exporting: {
                        enabled: false
                    },
                    credits: {
                        enabled: false
                    },
                    series: seriesOptions
                }, function (chart) {
                    var attrs = {
                        align: 'left',
                        zIndex: 20,
                        cursor: 'pointer'
                    };
                });

            panelChart.doLayout();
            return true;
        }
        SP.resizeCollectingStatusChart = function (component) {
            try {
                if (typeof(SP.collectingStatusChart) != 'undefined') {
                    SP.collectingStatusChart.setSize(component.getWidth(), component.getHeight(), false);
                }
            } catch(e) {}
        };
        SP.getCollectingStatusOptions = function () {
            return  {
                title: {
                    text: OM.dictionary.get('chartCollectingStatusInfoTitle')
                },
                xAxis: {
                    title: {
                        text: OM.dictionary.get('date')
                    },
                    labels: {
                        overflow: 'justify',
                        format: '{value: %d.%m.%Y}'
                    },
                    tickInterval: 1
                },
                yAxis: {
                    title: {
                        text: OM.dictionary.get('status'),
                        align: 'high'
                    },
                    min: 0,
                    max: 100
                },
                tooltip: {
                    formatter: function () {
                        var date = new Date(this.x);
                        return date.getDate() + '.' + (date.getMonth() + 1) + '.' + date.getFullYear() + ': ' + this.y.toString().replace('.', ',') + ' % ';
                    }

                },
                plotOptions: {
                    bar: {
                        dataLabels: {
                            enabled: true,
                            formatter: function () { return this.y.toString().replace('.', ',') + ' % ' }
                        }
                    },
                },
                chartType: 'bar'
            }
        }

        SP.getCollectingStatusColor = function (value) {
            if (value >= 100)
                return '#669535';
            if (value < 50)
                return 'indianred';
            return '#7CB5EC';
        }
    </script>


